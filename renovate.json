{
  "extends": [
    "config:recommended",
    "docker:enableMajor"
  ],
  "ignoreDeps": [
    "marksafe",
    "netbox_bgp"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "ignoreTests": true
}
